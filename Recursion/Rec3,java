public class Rec3 {
    public static int factorial(int n){
        if( n == 0 || n == 1 ){
            return 1;
        }
        int fact = factorial(n-1);
        int fn = n*factorial( n-1);
        return fn;
    }
    public static void main(String[] args) {
        System.out.println(factorial(5));
    }
}
